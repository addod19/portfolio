{
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "extends": ["airbnb-base",  "plugin:react/recommended"],
    "plugins": ["react"],
    "rules": {
      "react/jsx-filename-extension": ["warn", { "extensions": [".js", ".jsx"] }],
      "no-shadow": "off",
      "no-param-reassign": "off",
      "import/no-unresolved": "off",
      "no-console": "off",
      "eol-last": "off",
      "arrow-parens": "off"
    },
    "ignorePatterns": [
      "dist/",
      "build/"
    ]
  }